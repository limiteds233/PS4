<h2> Latest orders </h2>
<table class="table table-striped">
          <thead>
            <tr>
              <th>Name</th>
              <th>Phone</th>
              <th>Address</th>
              <th>Date</th>
              <th>Order</th>
            </tr>
          </thead>
          <tbody>

          <% @all_orders.each do |item| %>
            <tr>
              <td><%= item.name %></td>
              <td><%= item.phone %></td>
              <td><%= item.address %></td>
              <td><%= item.created_at %></td>
              <td><%= item.orders_input %></td>
            </tr>
          <% end %>
          </tbody>
</table>


